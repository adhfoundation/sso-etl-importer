generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model stg_user {
  id              Int              @id @default(autoincrement())
  primary_email   String?
  username        String?
  password        String?
  name            String?
  cpf             String?          @db.VarChar(255)
  created_at      DateTime         @default(now())
  stg_import_logs stg_import_log[]
  stg_phone       stg_phone[]
  stg_profile     stg_profile?
}

model stg_profile {
  id                 Int           @id @default(autoincrement())
  user_id            Int           @unique
  family_name        String?
  given_name         String?
  nickname           String?
  preferred_username String?
  gender             String?
  birthdate          DateTime?     @db.Date
  profile            String?
  website            String?
  zoneinfo           String?
  locale             String?
  created_at         DateTime      @default(now())
  stg_address        stg_address[]
  stg_user           stg_user      @relation(fields: [user_id], references: [id], onDelete: Cascade, onUpdate: NoAction)
}

model stg_address {
  id             Int         @id @default(autoincrement())
  user_id        Int
  formatted      String?
  street_address String?
  locality       String?
  region         String?
  postal_code    String?
  country        String?
  created_at     DateTime    @default(now())
  stg_profile    stg_profile @relation(fields: [user_id], references: [id], onDelete: Cascade, onUpdate: NoAction)
}

model stg_phone {
  id           Int      @id @default(autoincrement())
  user_id      Int
  phone        String?
  country_code String?
  prefix       String?
  number       String?
  created_at   DateTime @default(now())
  stg_user     stg_user @relation(fields: [user_id], references: [id], onDelete: Cascade, onUpdate: NoAction)
}

model stg_import_log {
  id             Int       @id @default(autoincrement())
  user_id        Int?
  type           String
  message        String
  created_at     DateTime  @default(now())
  index_register String?
  file           String?
  batch_id       String?
  stg_user       stg_user? @relation(fields: [user_id], references: [id], onDelete: Cascade, onUpdate: NoAction)
}
