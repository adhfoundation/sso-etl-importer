{
  "name": "carga",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "run:migrations ": "npx prisma migrate reset",
    "db:generate": "npx prisma generate",
    "db:pull": "npx prisma db pull",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:json": "ts-node -r tsconfig-paths/register src/main.ts --json --runBatch --preClear",
    "start:csv": "ts-node -r tsconfig-paths/register src/main.ts --csv --runBatch --preClear",
    "start:all": "ts-node -r tsconfig-paths/register src/main.ts --json --csv --runBatch --preClear",
    "start:clear": "ts-node -r tsconfig-paths/register src/main.ts --endClear",
    "start:batch": "ts-node -r tsconfig-paths/register src/main.ts --runOnlyBatch",
    "start:validation": "ts-node -r tsconfig-paths/register src/logto/streamUsers.ts"
  },
